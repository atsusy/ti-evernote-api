#import "TiProxy.h"
#import "<%= @basename %>.h"

@interface <%= s.proxy_class_name %> : TiProxy {
@private
    <%= s.class_name %>* object;
}
@property (nonatomic, retain) <%= s.class_name %>* object;

- (id)initWithObject:(<%= s.class_name %>*)thrift_object;
- (NSDictionary*)toHash:(id)args;

<% s.fields.each do |field| %>
- (id)<%= field.name.sub(/^[A-Z]/,&:downcase) %>;
- (void)set<%= field.name.sub(/^[a-z]/,&:upcase) %>:(id)value;
<% end %>
@end
